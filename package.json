{
  "name": "typescript-patterns",
  "version": "1.0.0",
  "description": "This repository contains a basic structure to learn good practices of Typescript, as well as some Design Patterns alongside the AWS SAM framework.",
  "dependencies": {
    "@graphql-codegen/cli": "^2.9.1",
    "@graphql-codegen/typescript": "^2.7.2",
    "@types/aws-lambda": "^8.10.92",
    "graphql": "^16.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.2",
    "husky": "^8.0.0"
  },
  "scripts": {
    "compile": "rm -rf dist && tsc",
    "generate-schema": "cd src/types/ && graphql-codegen",
    "quality": "npm audit && npm run lint && npm test",
