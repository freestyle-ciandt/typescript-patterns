{
  "compilerOptions": {
    "types": ["jest"],
    "outDir": "./dist",
    "target": "es2020",
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "module":"es2015",
    "moduleResolution":"node",
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,  
  },
  "include": ["src", "./test.ts"],
  "exclude": ["node_modules"]
}